<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>@ViewBag.Title</title>

    <link href="~/Css/bootstrap/bootstrap.css" rel="stylesheet" />

    <link href="~/Css/datatables/dataTables.bootstrap4.css" rel="stylesheet" />
    <link href="~/fontawesome-free/css/all.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">

    <link href="~/Css/admin/sb-admin-2.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="~/Css/style.css" />
</head>
<body id="page-top">
    <div id="wrapper">
        <div id="content-wrapper" class="d-flex flex-column">
            <div id="content">
                <partial name="~/Views/Shared/_Header.cshtml"/>
                <div class="container-fluid">
                    @RenderBody()
                </div>
            </div>
            <partial name="~/Views/Shared/_Footer.cshtml"/>
            <partial name="~/Views/Shared/Admin/_LogoutModal.cshtml"/>
        </div>
    </div>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.0/jquery.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.1/jquery.validate.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validation-unobtrusive/3.2.11/jquery.validate.unobtrusive.js"></script>
    <script src="~/Js/bootstrap/bootstrap.js"></script>
        
    <!-- Core plugin JavaScript-->
    <script src="~/Js/jquery-easing/jquery.easing.js"></script>
    <script src="~/Js/datatables/jquery.dataTables.js"></script>
            
    <!-- Custom scripts for all pages-->
    <script src="~/Js/admin/sb-admin-2.js"></script>
            
    <!-- Page level custom scripts -->
    <script src="~/js/demo/chart-area-demo.js"></script>
    <script src="~/js/demo/chart-pie-demo.js"></script>
</body>
</html>
